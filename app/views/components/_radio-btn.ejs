<fieldset class="form-group row js-radio">
<legend class="col-form-legend col-xs-12"><%= radioBtn.radioTopLabel %></legend>
<div class="col-sm-10">
  <% radioBtn.radio.forEach(function(item) { %>
  <div class="form-check radio-area <%= item.class %>">
    <label for="<%= item.radioId %>" class="form-check-label">
      <input
      type="radio"
      class="form-check-input js-validate <%= item.radioClass %>"
      name="<%= item.radioName %>"
      id="<%= item.radioId %>"
      value="<%= item.radioId %>"
      <%if ( item.dataFieldName ) { %> data-fieldName="<%= item.dataFieldName %>" <% } %>
      <%= item.required %> />
      <%- item.labelText %>
    </label>
  </div>
  <% }) %>
</div>
</fieldset>
