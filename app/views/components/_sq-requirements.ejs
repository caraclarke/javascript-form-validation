<ul class="req-checker col-xs-12 pd hide"
  aria-live="polite" aria-relevant="additions" aria-atomic="true" role="status"
  id="<%= info.roleId %>">
    <li class="req-checker_criteria js-allSqChecked js-SqChar">
      <span class="icon req-checker_criteria_icon icon--green-check"></span>
      <span aria-controls="<%= info.roleId %>" class="js-sq-char js-submitText">At least <strong>3 characters</strong>.</span>
      <span aria-controls="<%= info.roleId %>" class="is-hidden js-sq-char js-submitText">Requirement met: <strong>At least 3 characters</strong>.</span>
      <span aria-controls="<%= info.roleId %>" class="is-hidden js-sq-char js-submitText">Requirement not met: <strong>At least 3 characters</strong>.</span>
    </li>

    <li class="req-checker_criteria js-allSqChecked js-SqUn">
      <span class="icon req-checker_criteria_icon icon--green-check"></span>
      <span class="js-submitText js-un-match">Your security answer <strong>can't match your username</strong></span>
      <span class="js-submitText is-hidden js-un-match">Requirement met: your security answer <strong>can't match <br /><span class="pw-indent">your username</span></strong></span>
      <span class="js-submitText is-hidden js-un-match">Requirement not met: your security answer <strong>can't match <br /><span class="pw-indent">your username</span></strong></span>
    </li>

    <li class="req-checker_criteria js-allSqChecked js-SqPw">
      <span class="icon req-checker_criteria_icon icon--green-check"></span>
      <span class="js-submitText js-pw-match">Your security answer <strong>can't match your password</strong></span>
      <span class="js-submitText is-hidden js-pw-match">Requirement met: your security answer <strong>can't match <br /><span class="pw-indent">your password</span></strong></span>
      <span class="js-submitText is-hidden js-pw-match">Requirement not met: your security answer <strong>can't match <br /><span class="pw-indent">your password</span></strong></span>
    </li>

    <li class="req-checker_criteria js-sqCharCri charCri">
      <span class="js-submitText js-sp-char">Do not use <strong>these special characters:
        <span class="sr-only">period</span>
        &#46; &lt; &#42; &#37;</strong>
      </span>
      <span class="js-submitText is-hidden js-sp-char">Requirement met: do not use <strong>these special characters:
        <span class="sr-only">period</span>
        &#46; &lt; &#42; &#37;</strong>
      </span>
      <span class="js-submitText is-hidden js-sp-char">Requirement not met: do not use <strong>these special characters:
        <span class="sr-only">period</span>
        &#46; &lt; &#42; &#37;</strong>
      </span>
    </li>
</ul>
